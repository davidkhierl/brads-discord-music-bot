{
	"name": "frenny-discord-bots",
	"version": "0.1.0",
	"description": "Frenny Discord Bots ðŸ¤˜",
	"main": "build/index.js",
	"repository": "https://github.com/davidkhierl/frenny-discord-bots",
	"author": "David Khierl <hi@davidkhierkhierl.com>",
	"private": "true",
	"license": "MIT",
	"engines": {
		"node": ">=16"
	},
	"type": "module",
	"scripts": {
		"build": "tsc -p tsconfig.production.json && copyfiles ./sentry-release-name.txt ./build/",
		"dev": "yarn gen-sentry-release-name && nodemon -r dotenv/config ./src/index.ts",
		"start": "node -r dotenv/config build/index.js",
		"postinstall": "yarn gen-sentry-release-name",
		"deploy-commands": "node -r dotenv/config build/deploy-commands.js",
		"gen-sentry-release-name": "sentry-cli releases propose-version --silent > sentry-release-name.txt"
	},
	"dependencies": {
		"@discordjs/builders": "^1.0.0",
		"@discordjs/rest": "^1.0.1",
		"@prisma/client": "^4.1.0",
		"@sentry/integrations": "^7.8.0",
		"@sentry/node": "^7.8.0",
		"@sentry/tracing": "^7.8.0",
		"chalk": "^5.0.1",
		"discord-api-types": "0.36.3",
		"discord-music-player": "^9.0.2",
		"discord.js": "^14.0.3",
		"dotenv": "^16.0.1",
		"lodash": "^4.17.21",
		"tslib": "^2.4.0"
	},
	"devDependencies": {
		"@sentry/cli": "^2.5.0",
		"@sentry/types": "^7.8.0",
		"@trivago/prettier-plugin-sort-imports": "^3.3.0",
		"@types/lodash": "^4.14.182",
		"@types/node": "^18.6.1",
		"@typescript-eslint/eslint-plugin": "^5.31.0",
		"@typescript-eslint/parser": "^5.31.0",
		"copyfiles": "^2.4.1",
		"eslint": "^8.20.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"nodemon": "^2.0.19",
		"prettier": "^2.7.1",
		"prisma": "^4.1.0",
		"ts-node": "^10.9.1",
		"typescript": "^4.7.4"
	},
	"prisma": {
		"schema": "src/prisma/schema.prisma"
	}
}
